{
  "name": "node-pty",
  "description": "Fork pseudoterminals in Node.JS",
  "author": {
    "name": "Microsoft Corporation"
  },
  "version": "0.10.0",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./typings/node-pty.d.ts",
  "napi": {
    "name": "node-pty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/microsoft/node-pty.git"
  },
  "files": [
    "lib/",
    "scripts/",
    "native/",
    "src/",
    "deps/",
    "typings/",
    "Cargo.toml",
    "Cargo.lock",
    "build.rs",
    "node-pty.*.node"
  ],
  "homepage": "https://github.com/microsoft/node-pty",
  "bugs": {
    "url": "https://github.com/microsoft/node-pty/issues"
  },
  "keywords": [
    "pty",
    "tty",
    "terminal",
    "pseudoterminal",
    "forkpty",
    "openpty"
  ],
  "scripts": {
    "build": "tsc -b ./src/tsconfig.json",
    "build:native": "npm run build --prefix ./native",
    "watch": "tsc -b -w ./src/tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src/",
    "install": "node ./scripts/build-if-needed.js || npm run build:native",
    "test": "cross-env NODE_ENV=test mocha -R spec --exit lib/*.test.js",
    "posttest": "npm run lint",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.5.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.3.7",
    "cross-env": "^5.1.4",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "ps-list": "^6.0.0",
    "typescript": "^3.8.3"
  }
}
